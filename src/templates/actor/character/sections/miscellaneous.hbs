<div id="miscellaneous">
	<div class="header">{{localize "gurps.character.miscellaneous"}}</div>
	<div class="fieldblock">
		<div class="label">{{localize "gurps.character.created_date"}}</div>
		<div class="field">
			<input type="text" value="{{date system.created_date}}" disabled />
		</div>
		<div class="label">{{localize "gurps.character.modified_date"}}</div>
		<div class="field">
			<input type="text" value="{{date system.modified_date}}" disabled />
		</div>
		<div class="label">{{localize "gurps.character.player_name"}}</div>
		<div class="field">
			<input
				type="text"
				name="system.profile.player_name"
				data={{system.profile.player_name}}
				data-dtype="string"
				{{input_lock editing}}
			/>
		</div>
	</div>
</div>
