{
	"preset": "es-jest",
	"testEnvironment": "node",
	"modulePaths": ["<rootDir>", "<rootDir>/src", "<rootDir>/dist", "<rootDir>/types/foundry-pc-types"],
	"moduleNameMapper": {
		"^@assets/(.*)$": "<rootDir>/static/assets/$1",
		"^@data$": "<rootDir>/src/module/data/index.ts",
		"^@data/(.*)$": "<rootDir>/src/module/data/$1",
		"^@actor$": "<rootDir>/src/module/actor/index.ts",
		"^@actor/(.*)$": "<rootDir>/src/module/actor/$1",
		"^@item$": "<rootDir>/src/module/item/index.ts",
		"^@item/(.*)$": "<rootDir>/src/module/item/$1",
		"^@feature$": "<rootDir>/src/module/system/feature/index.ts",
		"^@feature/(.*)$": "<rootDir>/src/module/system/feature/$1",
		"^@prereq$": "<rootDir>/src/module/system/prereq/index.ts",
		"^@prereq/(.*)$": "<rootDir>/src/module/system/prereq/$1",
		"^@scene$": "<rootDir>/src/module/scene/index.ts",
		"^@scene/(.*)$": "<rootDir>/src/module/scene/$1",
		"^@system$": "<rootDir>/src/module/system/index.ts",
		"^@sytem/(.*)$": "<rootDir>/src/module/system/$1",
		"^@ui$": "<rootDir>/src/module/ui/index.ts",
		"^@ui/(.*)$": "<rootDir>/src/module/ui/$1",
		"^@module/(.*)$": "<rootDir>/src/module/$1",
		"^@scripts/(.*)$": "<rootDir>/src/scripts/$1",
		"^@util$": "<rootDir>/src/util/index.ts",
		"^@util/(.*)$": "<rootDir>/src/util/$1",
		"^@lang/(.*)$": "<rootDir>/static/lang/$1"
	},
	"setupFiles": ["./tests/setup.ts"]
}
